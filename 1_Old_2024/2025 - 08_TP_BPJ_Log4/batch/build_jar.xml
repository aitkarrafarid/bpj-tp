<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project 6_TP_Log_et_Properties with libraries in sub-folder">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="../batch/tp_log4j.jar">
            <manifest>
                <attribute name="Main-Class" value="tp_log4j.TpLog4j"/>
                <attribute name="Class-Path" value=". tp_log4j_lib/log4j-1.2.17.jar"/>
            </manifest>
            <fileset dir="../bin"/>
        </jar>
        <delete dir="../batch/tp_log4j_lib"/>
        <mkdir dir="../batch/tp_log4j_lib"/>
        <copy file="../lib/log4j-1.2.17.jar" todir="../batch/tp_log4j_lib"/>
    </target>
</project>
